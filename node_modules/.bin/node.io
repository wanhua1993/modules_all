#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_node.io@0.5.1@node.io/bin/node.io" "$@"
  ret=$?
else 
  node  "$basedir/../_node.io@0.5.1@node.io/bin/node.io" "$@"
  ret=$?
fi
exit $ret
